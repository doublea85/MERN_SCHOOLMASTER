}></Navigate>\n\n  return (\n    <div className=\"container mx-auto\">\n\n      <Toaster position='top-center' reverseOrder={false}></Toaster>\n\n      <div className='flex justify-center items-center h-screen'>\n        <div className={styles.glass} style={{ width : \"50%\"}}>\n\n          <div className=\"title flex flex-col items-center\">\n            <h4 className='text-5xl font-bold'>Reset</h4>\n            <span className='py-4 text-xl w-2/3 text-center text-gray-500'>\n              Enter new password.\n            </span>\n          </div>\n\n          <form className='py-20' onSubmit={formik.handleSubmit}>\n              <div className=\"textbox flex flex-col items-center gap-6\">\n                  <input {...formik.getFieldProps('password')} className={styles.textbox} type=\"text\" placeholder='New Password' />\n                  <input {...formik.getFieldProps('confirm_pwd')} className={styles.textbox} type=\"text\" placeholder='Repeat Password' />\n                  <button className={styles.btn} type='submit'>Reset</button>\n              </div>\n\n          </form>\n\n        </div>\n      </div>\n    </div>\n  )\n}\n","/Users/arcanum/Documents/GitHub/Classified/MERN_SCHOOLMASTER/client/src/components/PageNotFound.js",[],[],"/Users/arcanum/Documents/GitHub/Classified/MERN_SCHOOLMASTER/client/src/components/Profile.js",[],[],"/Users/arcanum/Documents/GitHub/Classified/MERN_SCHOOLMASTER/client/src/middleware/auth.js",["162","163"],[],"/Users/arcanum/Documents/GitHub/Classified/MERN_SCHOOLMASTER/client/src/helper/convert.js",[],[],"/Users/arcanum/Documents/GitHub/Classified/MERN_SCHOOLMASTER/client/src/store/store.js",[],[],"/Users/arcanum/Documents/GitHub/Classified/MERN_SCHOOLMASTER/client/src/hooks/fetch.hook.js",[],[],"/Users/arcanum/Documents/GitHub/Classified/MERN_SCHOOLMASTER/client/src/helper/helper.js",[],[],"/Users/arcanum/Documents/GitHub/Classified/MERN_SCHOOLMASTER/client/src/helper/validate.js",[],["164","165"],"/Users/arcanum/Documents/Gi